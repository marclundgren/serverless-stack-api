{
  "name": "serverless-stack-api",
  "version": "2.2.0",
  "description": "A free notes app",
  "scripts": {
    "test": "jest",
    "deploy": "serverless deploy",
    "run:script:create": "serverless invoke local --function create --path mocks/create-event.json",
    "run:script:get": "serverless invoke local --function get --path mocks/get-event.json",
    "run:script:list": "serverless invoke local --function list --path mocks/list-event.json",
    "run:script:update": "serverless invoke local --function update --path mocks/update-event.json",
    "run:script:delete": "serverless invoke local --function delete --path mocks/delete-event.json",
    "run:script:billing": "serverless invoke local --function billing --path mocks/billing-event.json"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marclundgren/serverless-stack-api.git"
  },
  "devDependencies": {
    "aws-sdk": "^2.768.0",
    "esm": "^3.2.25",
    "jest": "^26.5.2",
    "prettier": "2.1.2",
    "serverless-bundle": "^3.1.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-offline": "^6.8.0"
  },
  "dependencies": {
    "stripe": "^8.107.0",
    "uuid": "^7.0.3"
  }
}
